{
  "name": "novicell-webpack-frontend-default",
  "version": "1.0.0",
  "description": "Novicell Default Frontend Webpack Setup",
  "main": "src/scripts/app.ts",
  "scripts": {
    "build:prod": "webpack -p --env.NODE_ENV=production",
    "build:dev": "webpack --env.NODE_ENV=development",
    "start:dev": "webpack --watch --env.NODE_ENV=development",
    "start:prod:analyze": "webpack -p --env.NODE_ENV=production --env.USE_WEBPACK_ANALYZER=true",
    "start:dev:analyze": "webpack --env.NODE_ENV=development --env.USE_WEBPACK_ANALYZER=true"
  },
  "keywords": [
    "Novicell",
    "Webpack",
    "Frontend",
    "Html",
    "Typescript",
    "Javascript",
    "CSS",
    "PostCss",
    "Less",
    "Sass",
    "Scss"
  ],
  "author": "Christian Nicolaisen",
  "license": "ISC",
  "devDependencies": {
    "@types/copy-webpack-plugin": "^4.0.1",
    "@types/extract-text-webpack-plugin": "^3.0.0",
    "@types/html-webpack-plugin": "^2.28.0",
    "@types/node": "^8.0.46",
    "@types/webpack": "^3.0.13",
    "@types/webpack-bundle-analyzer": "^2.9.0",
    "@types/webpack-merge": "^4.1.0",
    "babel-core": "^6.26.0",
    "babel-loader": "^7.1.2",
    "babel-preset-env": "^1.6.1",
    "clean-webpack-plugin": "^0.1.17",
    "copy-webpack-plugin": "^4.2.0",
    "css-loader": "^0.28.7",
    "extract-text-webpack-plugin": "^3.0.1",
    "file-loader": "^1.1.5",
    "html-webpack-plugin": "^2.30.1",
    "image-webpack-loader": "^3.4.2",
    "less": "^3.0.0-alpha.3",
    "less-loader": "^4.0.5",
    "node-sass": "^4.5.3",
    "postcss": "^6.0.13",
    "postcss-cssnext": "^3.0.2",
    "postcss-import": "^11.0.0",
    "postcss-loader": "^2.0.8",
    "postcss-reporter": "^5.0.0",
    "sass-loader": "^6.0.6",
    "style-loader": "^0.19.0",
    "ts-loader": "^3.0.5",
    "ts-node": "^3.3.0",
    "tslint": "^5.8.0",
